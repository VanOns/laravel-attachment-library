{
  "name": "van-ons/laravel-attachment-library",
  "description": "A laravel library for attaching files to Eloquent models",
  "license": "MIT",
  "authors": [
    {
      "name": "Dangelo Brusorio",
      "email": "dangelo@van-ons.nl"
    },
    {
      "name": "Danique Wijnalda",
      "email": "danique@van-ons.nl"
    }
  ],
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.52",
    "larastan/larastan": "^2.9",
    "orchestra/testbench": "^8.21",
    "phpunit/phpunit": "^10.1"
  },
  "scripts" : {
    "check-lint": "./vendor/bin/php-cs-fixer fix --dry-run --diff",
    "lint": "./vendor/bin/php-cs-fixer fix",
    "phpstan": "./vendor/bin/phpstan analyse --memory-limit 2G"
  },
  "autoload": {
    "psr-4": {
      "VanOns\\LaravelAttachmentLibrary\\": "src",
      "Database\\Factories\\": "database/factories/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VanOns\\LaravelAttachmentLibrary\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VanOns\\LaravelAttachmentLibrary\\LaravelAttachmentLibraryServiceProvider"
      ]
    }
  },
  "config" : {
    "sort-packages": true
  }
}
