{
  "name": "van-ons/laravel-attachment-library",
  "description": "A laravel library for attaching files to Eloquent models",
  "license": "MIT",
  "authors": [
    {
      "name": "Dangelo Brusorio",
      "email": "dangelo@van-ons.nl"
    },
    {
      "name": "Danique Wijnalda",
      "email": "danique@van-ons.nl"
    }
  ],
  "require": {},
  "require-dev": {
    "orchestra/testbench": "^8.21",
    "phpunit/phpunit": "^10.1"
  },
  "autoload": {
    "psr-4": {
      "VanOns\\LaravelAttachmentLibrary\\": "src",
